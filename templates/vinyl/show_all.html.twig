{% extends 'base.html.twig' %}

{% block title %}Create a new Record | {{ parent() }}{% endblock %}

{% block body %}
    <h1>All tracks:</h1>
    {{ dump() }}
    <ul>
        {% for track in tracks %}
            <li>{{ track.title }} - {{ track.singer|upper }}</li>
        {% endfor %}
    </ul>

    <a href="{{ path('browse', {slug: 'hey-you'}) }}">hey-you</a>
    <a href="{{ path('browse', {slug: 'water-melon'}) }}">water-melon</a>
    <a href="{{ path('browse', {slug: 'ice-cream'}) }}">ice-cream</a>
    <a href="{{ path('browse') }}">nothing</a>

{% endblock %}
